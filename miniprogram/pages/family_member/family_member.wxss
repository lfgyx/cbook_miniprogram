.category {
  background: #f5f5f5;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
}

.cateBox {
  padding-bottom: 200px;
}

.cate_list {
  width: 95%;
  height: 60px;
  margin: 10px;
  background: #fff;
  border-radius: 10px;
  position: relative;
}

.cate_list:last-child {
  margin-bottom: 100px;
}

.cate_icon {
  width: 20px;
  display: inline-block;
  margin: 0 20px 0 25px;

}

.cate_icon image {
  width: 25px;
  height: 25px;
  margin: 16px 0 20px 0;
  vertical-align: middle;
}

.cate_title {
  line-height: 60px;
  display: inline-block;
}

.add_btn {
  width: 24px;
  height: 24px;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -12px;
  margin-top: -12px;
}

.add_cate_btn {
  position: fixed;
  right: 30px;
  bottom: 60px;
  width: 50px;
  height: 50px;
  background-color: #fff;
  border-radius: 50%;
  box-shadow: 0 8rpx 20rpx rgba(0, 0, 0, 0.2);
}


/* index.wxss */
.container {
  padding: 20px;
}


.input-box {
  margin-bottom: 10px;
  padding-top: 10px;
}

.input-box input {
  border: 1px solid #ddd;
  border-radius: 5px;
  padding: 10px;
}

.delete_btn {
  float: right;
  width: 24px;
  height: 24px;
  margin-right: 28px;
  margin-top: 18px;
}

.delete_btn image {
  width: 24px;
  height: 24px;
}